//表单
.wdu-form-label{
	font-size: $input-fs;float:left;padding: $input-padding;height: $input-height; line-height: $input-height;text-align: right;
}
.wdu-form-text{
	font-size: $input-fs;float:left;padding: 5px 0;height: $input-height; line-height: $input-height;text-align: left;
}
.wdu-form-input{
	border: 1px solid $input-cb;
	padding: $input-padding;
	height: $input-height;
	line-height: $input-height;
	border-radius: $input-border-radius;
	font-size: $input-fs;
	float:left;
	&:disabled{background-color: $input-cbg-disabled; color:$input-color-disabled;border: 1px solid $input-cb !important;}
	&:hover{border: 1px solid $input-cb-hover;}

}
.wdu-form-input-search:disabled{background-color: transparent;}
.wdu-form-textarea{
	border: 1px solid $textarea-cb;
	padding: $textarea-padding;
	min-height: $textarea-height;
	line-height: $textarea-height;
	border-radius: $textarea-border-radius;
	font-size: $textarea-fs;
	float:left;
}
.wdu-btn-search{
	float:left;
	position: relative;
	z-index: 2;
	padding:7px 8px;
	margin-left: -1px;
	border-radius: 0 2px 2px 0;
	.icon-search{display: block;}
	.icon-search-white{display: block;}
}