//图片滚动左右按钮
.wdu-pre-scroll,.wdu-next-scroll{background-image: url('#{$path}');position: absolute;top: 50%;transform: translateY(-50%);}
.wdu-pre-scroll{left:12px;@extend .icon-pre;}
.wdu-next-scroll{right:12px;@extend .icon-next;}
.wdu-delete-scroll{position: absolute;top:-11px;right: -11px;}
//按钮
.wdu-btn,.wdu-btn-reverse,.wdu-btn-reverse-gray,.wdu-btn-gray{
	text-align: center;
	border-radius: $btn-border-radius;
}
.wdu-btn{
	color:$btn-color;
	border:1px solid $btn-cb;
	background-color: $btn-cbg;
	$transition:background-color .2s;
	&:hover{
		background-color: $btn-cbg-hover;
		$transition:background-color .2s;
	}
	&:active{
		background-color: $btn-cbg-active;
	}
	&:disabled{
		background-color: $btn-cbg-disabled;
	}
}
.wdu-btn-reverse{
	color:$btn-color-reverse;
	border: 1px solid $btn-cb-reverse;
	background-color: $btn-cbg-reverse;
	$transition:border-color .2s,color .2s;
	&:hover{
		border-color: $btn-cb-reverse-hover;
		color: $btn-color-reverse-hover;
		$transition:border-color .2s,color .2s;
	}
	&:active{
		border-color: $btn-cb-reverse-active;
		color: $btn-color-reverse-active;
	}
	&:disabled{
		border-color: $btn-cb-reverse-disabled;
		color: $btn-color-reverse-disabled;
	}
}
.wdu-btn-reverse-gray{
	color:$btn-color-reverse-gray;
	border: 1px solid $btn-cb-reverse-gray;
	background-color: $btn-cbg-reverse-gray;
	$transition:border-color .2,color .2s;
	&:hover{
		border-color: $btn-cb-reverse-gray-hover;
		color: $btn-color-reverse-gray-hover;
		$transition:border-color .2,color .2s;
	}
	&:active{
		border-color: $btn-cb-reverse-gray-active;
		color: $btn-color-reverse-gray-active;
	}
	&:disabled{
		border-color: $btn-cb-reverse-gray-disabled;
		color: $btn-color-reverse-gray-disabled;
	}
}
.wdu-btn-gray{
	color:$btn-color-gray;
	border: 1px solid $btn-cb-gray;
	background-color: $btn-cbg-gray;
	$transition:background-color .2s;
	&:hover{
		background-color: $btn-cbg-gray-hover;
		$transition:background-color .2s;
	}
	&:active{
		background-color: $btn-cbg-gray-active;
	}
	&:disabled{
		background-color: $btn-cbg-gray-disabled;
		color:$btn-color-gray-disabled;
	}
}
.wdu-btn-lg{
	font-size: $btn-fs-lg;
	line-height: $btn-line-height-lg;
	padding: $btn-padding-lg;

}
.wdu-btn-md{
